{
    "telemetry.enableTelemetry": false,
    "nxConsole.enableTelemetry": false,
    "workbench.colorTheme": "Material Theme Lighter High Contrast",
    "workbench.iconTheme": "material-icon-theme",
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "Material Theme Lighter High Contrast",
    "workbench.preferredDarkColorTheme": "Material Theme Ocean High Contrast",
    "material-icon-theme.folders.associations": {
        "global_state": "global",
        "ui": "layout",
        "bloc": "controller",
        "features": "app",
        "presentation": "layout",
        "domain": "rules",
        "entities": "class"
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "[*]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "dart.previewFlutterUiGuides": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintArgs": [
        "--enable=F,E,W"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "settingsSync.ignoredSettings": [],
    "settingsSync.ignoredExtensions": [],
    "editor.suggestSelection": "first",
    "files.eol": "\n",
    "cSpell.userWords": [
        "Linkedin",
        "Losho",
        "MAILGUN",
        "Mosho",
        "Oshte",
        "Penchev",
        "Postgres",
        "RABBITMQ",
        "TIMESTAMPTZ",
        "VARCHAR",
        "arange",
        "camelcase",
        "conv",
        "dartz",
        "deque",
        "drun",
        "dtype",
        "ehrer",
        "gosho",
        "jsonify",
        "keras",
        "messagebird",
        "mockito",
        "mutex",
        "ndarray",
        "numpy",
        "posho",
        "prefetch",
        "relu",
        "respawn",
        "slonik",
        "sqlite",
        "twilio",
        "typeorm",
        "usecase",
        "usecases",
        "uuidv",
        "wordlist",
        "wordlists"
    ]
}
