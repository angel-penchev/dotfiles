{
    "telemetry.telemetryLevel": "off",
    "nxConsole.enableTelemetry": false,
    "workbench.iconTheme": "material-icon-theme",
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "Community Material Theme Darker High Contrast",
    "workbench.preferredLightColorTheme": "Material Theme Lighter High Contrast",
    "workbench.preferredDarkColorTheme": "Community Material Theme Darker High Contrast",
    "material-icon-theme.folders.associations": {
        "global_state": "global",
        "ui": "layout",
        "bloc": "controller",
        "features": "app",
        "presentation": "layout",
        "domain": "rules",
        "entities": "class"
    },
    "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono Retina",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "[*]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "dart.previewFlutterUiGuides": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintArgs": [
        "--enable=F,E,W"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "settingsSync.ignoredSettings": [],
    "settingsSync.ignoredExtensions": [],
    "editor.suggestSelection": "first",
    "files.eol": "\n",
    "vscode-neovim.neovimExecutablePaths.win32": "C:/tools/neovim/Neovim/bin/nvim.exe",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "cSpell.userWords": [
        "Linkedin",
        "Losho",
        "MAILGUN",
        "Mosho",
        "Oshte",
        "Penchev",
        "Postgres",
        "RABBITMQ",
        "TIMESTAMPTZ",
        "VARCHAR",
        "arange",
        "camelcase",
        "conv",
        "dartz",
        "deque",
        "drun",
        "dtype",
        "ehrer",
        "gosho",
        "jsonify",
        "keras",
        "messagebird",
        "mockito",
        "mutex",
        "ndarray",
        "numpy",
        "posho",
        "prefetch",
        "relu",
        "respawn",
        "slonik",
        "sqlite",
        "twilio",
        "typeorm",
        "usecase",
        "usecases",
        "uuidv",
        "wordlist",
        "wordlists"
    ],
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#AF1C38",
        "activityBar.activeBorder": "#AF1C38",
        "list.activeSelectionForeground": "#AF1C38",
        "list.inactiveSelectionForeground": "#AF1C38",
        "list.highlightForeground": "#AF1C38",
        "scrollbarSlider.activeBackground": "#AF1C3850",
        "editorSuggestWidget.highlightForeground": "#AF1C38",
        "textLink.foreground": "#AF1C38",
        "progressBar.background": "#AF1C38",
        "pickerGroup.foreground": "#AF1C38",
        "tab.activeBorder": "#AF1C38",
        "notificationLink.foreground": "#AF1C38",
        "editorWidget.resizeBorder": "#AF1C38",
        "editorWidget.border": "#AF1C38",
        "settings.modifiedItemIndicator": "#AF1C38",
        "settings.headerForeground": "#AF1C38",
        "panelTitle.activeBorder": "#AF1C38",
        "breadcrumb.activeSelectionForeground": "#AF1C38",
        "menu.selectionForeground": "#AF1C38",
        "menubar.selectionForeground": "#AF1C38",
        "editor.findMatchBorder": "#AF1C38",
        "selection.background": "#AF1C3840",
        "statusBarItem.remoteBackground": "#AF1C38"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "redhat.telemetry.enabled": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "sonarlint.rules": {
        "typescript:S4326": {
            "level": "off"
        }
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[nginx]": {
        "editor.defaultFormatter": "raynigon.nginx-formatter"
    }
}